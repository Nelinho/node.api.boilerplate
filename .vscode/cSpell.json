        "dbtest",
        "deserialize",
        "esnext",
        "evenodd",
        "farmhash",
        "fieldname",
        "Guid",
        "kcors",
        "keepalive",
        "lspace",
        "middlewares",
        "mockito",
        "msisdn",
        "Msisdns",
        "nacl",
        "nbsp",
        "nospace",
        "onespace",
        "outro",
        "pree",
        "preheader",
        "promisify",
        "quotemark",
        "rspace",
        "sbin",
        "sendmail",
        "testdb",
        "tweetnacl",
        "typedi",
        "typeorm",
        "valign",
        "Websockets",
        "whitelisted",
        "xlink",
        "xmlns"
